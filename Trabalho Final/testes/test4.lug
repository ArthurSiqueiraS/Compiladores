main {
  var int i;
  var int num;
  var bool cond;

  i := 0;
  do {
    cond := isOdd(i);
    if(cond) {
      num := (i * 2);
      print(num);
    };
    i := (i + 1);
  } while((i < 10));

  endProgram();
}

function bool isOdd(int num) {
  while((num > 2)) do {
    num := (num - 2);
  };
  return (num == 1);
}

function int endProgram() {
  print(true);
}